<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="190.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlador.ControladorLogin">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Usuario">
      <VBox.margin>
         <Insets right="100.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoUsuario" promptText="Usuario"/>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="IP">
      <VBox.margin>
         <Insets right="125.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoServidor" promptText="Servidor"/>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Puerto">
      <VBox.margin>
         <Insets right="100.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoPuerto" promptText="Puerto"/>
   <Button onAction="#inicioSesion" text="Connect" />
</VBox>
