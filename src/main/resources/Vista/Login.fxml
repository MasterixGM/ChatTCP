<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="403.0" prefWidth="227.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.infantaelena.ies.Controlador.ControladorLogin">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="Usuario" textOrigin="CENTER">
      <VBox.margin>
         <Insets right="135.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoUsuario" promptText="Usuario" />
   <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="Contraseña" textOrigin="CENTER">
      <VBox.margin>
         <Insets right="120.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoContrasena" promptText="Contraseña" />
   <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="IP" textOrigin="CENTER">
      <VBox.margin>
         <Insets right="163.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoServidor" promptText="Servidor">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
   <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="Puerto" textOrigin="CENTER">
      <VBox.margin>
         <Insets right="145.0" />
      </VBox.margin>
   </Text>
   <TextField fx:id="campoPuerto" promptText="Puerto">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
   <Button onAction="#inicioSesion" text="Connect">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin></Button>
</VBox>
