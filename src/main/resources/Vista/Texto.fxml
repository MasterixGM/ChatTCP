<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="75.0" styleClass="todo" stylesheets="@../CSS/Texto.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.infantaelena.ies.Controlador.ControladorTexto">
   <children>
      <VBox prefHeight="30.0" prefWidth="100.0" styleClass="todo">
         <children>
            <Label fx:id="labelCliente" styleClass="texto" text="Label">
               <opaqueInsets>
                  <Insets  left="10.0" right="5.0" top="10.0" />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="labelMensaje" styleClass="texto" text="Label" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="30.0" right="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
